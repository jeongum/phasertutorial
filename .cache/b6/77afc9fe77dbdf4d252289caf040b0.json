urce":"src/scenes/MenuScene.js","original":{"line":50,"column":8}},{"generated":{"line":82,"column":33},"source":"src/scenes/MenuScene.js","original":{"line":50,"column":34}},{"generated":{"line":82,"column":45},"source":"src/scenes/MenuScene.js","original":{"line":50,"column":38}},{"name":"console","generated":{"line":83,"column":0},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":12}},{"name":"console","generated":{"line":83,"column":8},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":12}},{"generated":{"line":83,"column":15},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":19}},{"name":"log","generated":{"line":83,"column":16},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":20}},{"generated":{"line":83,"column":19},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":12}},{"generated":{"line":83,"column":20},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":24}},{"generated":{"line":83,"column":35},"source":"src/scenes/MenuScene.js","original":{"line":51,"column":12}},{"generated":{"line":84,"column":0},"source":"src/scenes/MenuScene.js","original":{"line":52,"column":9}},{"generated":{"line":84,"column":7},"source":"src/scenes/MenuScene.js","original":{"line":50,"column":8}},{"generated":{"line":85,"column":0},"source":"src/scenes/MenuScene.js","original":{"line":54,"column":5}},{"name":"Phaser","generated":{"line":89,"column":2},"source":"src/scenes/MenuScene.js","original":{"line":2,"column":31}},{"generated":{"line":89,"column":8},"source":"src/scenes/MenuScene.js","original":{"line":2,"column":37}},{"name":"Scene","generated":{"line":89,"column":9},"source":"src/scenes/MenuScene.js","original":{"line":2,"column":38}}],"sources":{"src/scenes/MenuScene.js":"import{CST} from\"../CST\"\r\nexport class MenuScene extends Phaser.Scene{\r\n    constructor(){\r\n        super({\r\n            key:CST.SCENES.MENU\r\n        })\r\n    }\r\n    init(data){\r\n        console.log(data);\r\n        console.log(\"I GOT IT\");\r\n    }\r\n    create(){\r\n        //setDepth is back or front 1 is always front, 0 is back\r\n        this.add.image(this.game.renderer.width/2, this.game.renderer.height * 0.20, \"logo\").setDepth(1);\r\n        this.add.image(0,0,\"title_bg\").setOrigin(0).setDepth(0);\r\n        let playButton = this.add.image(this.game.renderer.width/2, this.game.renderer.height/2, \"play_button\").setDepth(1);\r\n        this.add.image(this.game.renderer.width/2, this.game.renderer.height/2 + 100, \"options_button\").setDepth(1);\r\n        \r\n        let hoverSprite=this.add.sprite(100,100,\"cat\");\r\n        hoverSprite.setScale(2);\r\n        hoverSprite.setVisible(false);\r\n\r\n        this.sound.play(\"title_music\",{\r\n            loop : true\r\n        })\r\n\r\n        this.anims.create({\r\n            key: \"walk\",\r\n            frameRate:4,\r\n            repeat: -1, //repeat forever\r\n            frames: this.anims.generateFrameNumbers(\"cat\",{\r\n                frames: [0,1,2,3]\r\n            })\r\n        })\r\n        playbutts.on();\r\n        playButton.setInteractive();\r\n\r\n        playButton.on(\"pointerover\",()=>{\r\n            hoverSprite.setVisible(true);\r\n            hoverSprite.play(\"walk\");\r\n            hoverSprite.x = playButton.x -playButton.width\r\n            hoverSprite.y = playButton.y;\r\n        })\r\n\r\n        playButton.on(\"pointerout\",()=>{\r\n            hoverSprite.setVisible(false);\r\n            console.log(\"OUTAA HERE\")\r\n        })\r\n\r\n        playButton.on(\"pointerup\",()=>{\r\n            console.log(\"OPEN THE GATE\")\r\n        })\r\n\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"769c523135182fdbbb3443ff2359096b","cacheData":{"env":{}}}